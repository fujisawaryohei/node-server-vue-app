{
  "compilerOptions": {
    "target": "ES2016",
    "module": "COMMONJS",
    "outDir": "./dist/",
    "rootDir": "./src/",
    "strict": true,
    "types": ["node"],
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/**/*"
   ]
}
